<h1> <%= @article.title %> </h1>
<p> <%= @article.body %> </p>
<%= link_to "Back to all articles", articles_path %> |
<%= link_to "Edit Article", edit_article_path(@article) %> |

<%= link_to "Delete Article", article_path(@article), method: :delete,
                  data: { confirm: "Do you want to delete this article?" } %>
